{
	"semi": false,
	"singleQuote": true,
	"useTabs": true,
	"tabWidth": 2,
	"bracketSpacing": true,
	"trailingComma": "es5",
	"htmlWhitespaceSensitivity": "css",
	"arrowParens": "avoid",
	"plugins": [
		"prettier-plugin-svelte"
	],
	"pluginSearchDirs": false,
	"svelteSortOrder": "options-styles-scripts-markup",
	"svelteStrictMode": false,
	"svelteBracketNewLine": true,
	"svelteIndentScriptAndStyle": true
}
